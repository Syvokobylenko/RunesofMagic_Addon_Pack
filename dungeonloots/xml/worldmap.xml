	<Button name="DL_WorldMap_Setting_DropdownButton" inherits="UIPanelButtonTemplate" parent="WorldMapViewFrame" text = "UI_WORLDMAP_SETUP">
		<Size>
			<AbsDimension x="120" y="22"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT">
				<Offset>
					<AbsDimension x="20" y="-22"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Scripts>
			<OnClick>
				ToggleDropDownMenu( DL_WorldMap_Setting_Dropdown, 1, nil, this:GetName(), 1 ,1 )				
