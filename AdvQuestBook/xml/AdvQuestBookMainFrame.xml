	<Button name="AdvQuestBookMiniMapShareButton" hidden="true" inherits="MinimapButtonTemplate" parent="UIParent">
		<Size>
			<AbsDimension y="20" x="20" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativeTo="MinimapViewFrame" relativePoint="CENTER">
				<Offset>
					<RelDimension x="-26" y="-72" />
				</Offset>
			</Anchor>
		</Anchors>
		<Scripts>
			<OnMouseDown>
				AdvQuestBookShareMiniButton_OnClick(this, key);
			</OnMouseDown>
			<OnEnter>
				GameTooltip:SetOwner(this, "TOPLEFT" and "ANCHOR_LEFT", -6, 0 or "TOPRIGHT" and "ANCHOR_RIGHT", 0, 6);
				GameTooltip:SetText("|cFF06ff00"..AdvQuestBook_Messages["AQB_GET_PQUEST"].."|r");
				GameTooltip:AddLine("|cFF00b1e0"..AdvQuestBook_Messages["AQB_SHIFT_RIGHT"].."|r");
				GameTooltip:Show();
			</OnEnter>
			<OnLeave>
				GameTooltip:ClearAllAnchors();
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
		<NormalTexture file="Interface\AddOns\AdvQuestBook\images\share.png" />
		<PushedTexture file="Interface\AddOns\AdvQuestBook\images\share.png" />
		<HighlightTexture file="Interface\AddOns\AdvQuestBook\images\sharehl.png" alphaMode="NONE" />
	</Button>
	<Frame name="AdvQuestBookMiniBookFrame" hidden="true" parent="UIParent" frameStrata="LOW" frameLevel="1002">
		<Size>
			<AbsDimension x="36" y="36" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER" />
		</Anchors>
		<Frames>
			<Frame name="$parentBorder" frameStrata="BACKGROUND">
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" />
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" />
				</Anchors>
				<Backdrop edgeFile="Interface\Tooltips\Tooltip-border">
					<EdgeSize>
						<AbsValue val="16" />
					</EdgeSize>
				</Backdrop>
			</Frame>
			<Frame name="$parentCorner" frameStrata="LOW" enableMouse="true">
				<Size>
					<AbsDimension x="16" y="16" />
				</Size>
				<Backdrop bgFile="Interface\WorldMap\WorldMap-TitlebarSwitch-Normal">
					<TileSize>
						<AbsValue val="4" />
					</TileSize>
				</Backdrop>
				<Scripts>
					<OnEnter>
						GameTooltip:ClearAllAnchors();
						GameTooltip:SetOwner(this, "TOPLEFT" and "ANCHOR_LEFT", -6, 0 or "TOPRIGHT" and "ANCHOR_RIGHT", 0, 6);
						GameTooltip:SetText("|cffFFFF00"..AdvQuestBook_Messages["AQB_MOVE_BTN"].."|r");
						GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						GameTooltip:ClearAllAnchors();
						GameTooltip:Hide();
					</OnLeave>
					<OnMouseDown>
						if (AdvQuestBook_Config["minibook"]["status"] == "unlocked") then
							AdvQuestBookMiniBookFrame:StartMoving("TOPLEFT");
						end
					</OnMouseDown>
					<OnMouseUp>
						if (AdvQuestBook_Config["minibook"]["status"] == "unlocked") then
							AdvQuestBookMiniBookFrame:StopMovingOrSizing();
							AdvQuestBookMiniButton_SavePOS();
						end
					</OnMouseUp>
				</Scripts>
			</Frame>
			<Button name="AdvQuestBookMiniBookButton" inherits="UIPanelButtonTemplate">
				<Size>
					<AbsDimension y="24" x="24" />
				</Size>
				<Anchors>
					<Anchor point="CENTER" relativePoint="CENTER" />
				</Anchors>
				<Scripts>
					<OnClick>
						AdvQuestBookMiniBook_OnClick(this, key);
					</OnClick>
					<OnEnter>
						PlaySoundByPath("sound\\interface\\ui_contextmenu_click.mp3");
						GameTooltip:ClearAllAnchors();
						GameTooltip:SetOwner(this, "TOPLEFT" and "ANCHOR_LEFT", -6, 0 or "TOPRIGHT" and "ANCHOR_RIGHT", 0, 6);
						GameTooltip:SetText("|cffFFFF00"..AdvQuestBook_Messages["AQB_OPEN"].." "..AQB_ADDON_NAME.."|r");
						if (AdvQuestBook_Config["minibook"]["status"] == "unlocked") then
							GameTooltip:AddLine(AdvQuestBook_Messages["AQB_RCLICK"].." - "..AdvQuestBook_Messages["AQB_LOCK"]);
						else
							GameTooltip:AddLine(AdvQuestBook_Messages["AQB_RCLICK"].." - "..AdvQuestBook_Messages["AQB_UNLOCK"]);
						end
						GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						GameTooltip:ClearAllAnchors();
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
				<NormalTexture file="Interface\AddOns\AdvQuestBook\images\aqb.png" />
				<PushedTexture file="Interface\AddOns\AdvQuestBook\images\aqb.png" />
				<HighlightTexture file="Interface\AddOns\AdvQuestBook\images\aqbhl.png" alphaMode="NONE" />
			</Button>
		</Frames>
	</Frame>
