					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-80" y="7" />
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						if (AdvQuestBookConfig:IsVisible()) then
							AdvQuestBookConfig:Hide();
						else
							if (AdvQuestBookIconConfig:IsVisible()) then
								AdvQuestBookIconConfig:Hide();
							end
							AdvQuestBookConfig:Show();
						end
					</OnClick>
				</Scripts>
