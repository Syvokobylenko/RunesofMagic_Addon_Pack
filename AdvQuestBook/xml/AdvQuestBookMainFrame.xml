			<Button name="$parentConfigButton2" inherits="UIPanelButtonTemplate" text="Icon Config" parent="AdvQuestBookMainFrame" frameLevel="2">
				<Size>
					<AbsDimension x="80" y="16" />
				</Size>
				<Anchors>
					<Anchor point="RIGHT" relativeTo="AdvQuestBookMainFrameConfigButton">
						<Offset>
							<AbsDimension x="-86" y="0" />
