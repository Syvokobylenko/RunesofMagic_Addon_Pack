			<Button name="$parentConfigButton" inherits="UIPanelButtonTemplate" text="AQB_XML_CONFIG" parent="AdvQuestBookMainFrame" frameLevel="2">
				<Size>
					<AbsDimension x="80" y="16" />
				</Size>
