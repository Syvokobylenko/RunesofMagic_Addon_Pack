<Ui xmlns="http://www.runewaker.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.runewaker.com/UI.xsd">
	<Button name="AdvQuestBookMapIndexIcon" hidden="true" enableMouse="true" virtual="true">
		<Size>
			<AbsDimension x="12" y="12" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER" />
		</Anchors>
		<NormalTexture file="Interface\AddOns\AdvQuestBook\images\index.png" />
		<PushedTexture file="Interface\AddOns\AdvQuestBook\images\index.png" />
		<HighlightTexture file="Interface\AddOns\AdvQuestBook\images\chl.png" alphaMode="NONE" />
		<Scripts>
			<OnClick>
				AdvQuestBook_AutoMoveTo(this:GetName());
			</OnClick>
			<OnEnter>
				AdvQuestBookIcon_OnDirection(this:GetName(), "idx", "in");
			</OnEnter>
			<OnLeave>
				AdvQuestBookIcon_OnDirection(this, nil, nil);
			</OnLeave>
		</Scripts>
	</Button>
