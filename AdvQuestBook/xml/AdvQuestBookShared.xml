<Ui xmlns="http://www.runewaker.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.runewaker.com/UI.xsd">
	<Button name="AdvQuestBookMapSharedIcon" hidden="true" enableMouse="true" virtual="true">
		<Size>
			<AbsDimension x="24" y="24" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER" />
		</Anchors>
		<NormalTexture file="Interface\AddOns\AdvQuestBook\images\shared.png" />
		<PushedTexture file="Interface\AddOns\AdvQuestBook\images\shared.png" />
		<HighlightTexture file="Interface\AddOns\AdvQuestBook\images\fhl.png" alphaMode="NONE" />
		<Scripts>
			<OnClick>
				AdvQuestBook_eAutoMoveToByNPCID(this:GetName());
			</OnClick>
			<OnEnter>
				AdvQuestBookIcon_OnDirection(this:GetName(), "sha", "in");
			</OnEnter>
			<OnLeave>
				AdvQuestBookIcon_OnDirection(this, nil, nil);
			</OnLeave>
		</Scripts>
	</Button>
