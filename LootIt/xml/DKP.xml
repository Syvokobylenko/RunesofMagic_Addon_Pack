					</OnEnterPressed>
					<OnHide>
						this:ClearFocus()
					</OnHide>
					<OnKeyUp>
						if this:GetText() ~= "" then
							LI.PaintMasterFilterFrame(this:GetText())
						else
							LI.PaintMasterFilterFrame()
						end
					</OnKeyUp>
					<OnEnter>
						LI.FilterExplain(this)
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide()
					</OnLeave>
				</Scripts>
