			<Frame name="$parent_Dropdown" inherits="UIDropDownMenuTemplate" hidden="true"/>
		</Frames>
		<Scripts>
			<OnClick>
				GameTooltip:Hide()

				ToggleDropDownMenu(LootIt_MasterLootButton_Dropdown)
			</OnClick>
