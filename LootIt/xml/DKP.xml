					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="12" y="14"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnEnter>
						GameTooltip:SetOwner(this, "ANCHOR_RIGHT")
						GameTooltip:SetText(GROUPLOOT_CANCEL, 1, 1, 1)
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide()
					</OnLeave>
