					<Size>
						<AbsDimension x="32" y="16"/>
					</Size>
					<Anchors>
						<Anchor point="BOTTOM">
							<Offset>
								<AbsDimension x="0" y="1"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<HighlightTexture file="Interface\Buttons\QuickSlot-Highlight" alphaMode="ADD"/>
		<CheckedTexture file="Interface\Buttons\QuickSlot-Checked" alphaMode="ADD"/>
	</CheckButton>
	<CheckButton name="EMI_MacroFrameButtonTemplate" inherits="EMI_MacroButtonTemplate" virtual="true">
		<Scripts>
			<OnEnter>
				local icon, name, body = GetMacroInfo(this:GetID())
				if HasMacro(this:GetID()) and name ~="" then
					GameTooltip:SetOwner(this, "ANCHOR_TOPLEFT", 0, 0);
					GameTooltip:SetText(name);
					GameTooltip:Show();
				end
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide()
			</OnLeave>
			<OnClick>
				ExtendedMacroIcons.func.MacroFrame.OnClick(this);
			</OnClick>
			<OnDragStart>
				PickupMacroItem(this:GetID());
			</OnDragStart>
			<OnReceiveDrag>
				PickupMacroItem(this:GetID());
				ExtendedMacroIcons.func.MacroFrame.Update()
			</OnReceiveDrag>
		</Scripts>
	</CheckButton>


	<CheckButton name="EMI_MacroPopupIconButtonTemplate" inherits="EMI_MacroButtonTemplate" virtual="true">
		<Scripts>
			<OnClick>
				ExtendedMacroIcons.func.MacroPopupIconButton.OnClick(this);
			</OnClick>
			<OnMouseWheel>
				UIPanelScrollBar_OnMouseWheel(EMI_MacroPopupIconScrollBar, delta);
			</OnMouseWheel>
			<OnEnter>
				if IsShiftKeyDown() then
					GameTooltip:SetOwner(this, "ANCHOR_TOPLEFT", 0, 0)
					GameTooltip:SetText(this.path)
					GameTooltip:Show()
				end
			</OnEnter>
			<OnLeave>
			GameTooltip:Hide()
			</OnLeave>

		</Scripts>
	</CheckButton>



