							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>Charplan.Search.HeaderClicked(this)</OnClick>
		</Scripts>
		<NormalText inherits="GameFontYellow"/>
