		</Layer>
	</Layers>
    <Scripts>
        <OnShow>
            yGather.minimapHighlight.OnShow(this);
        </OnShow>
        <OnLoad>
            yGather.minimapHighlight.OnLoad(this);
        </OnLoad>
        <OnUpdate>
            yGather.minimapHighlight.OnUpdate(this, elapsedTime);
        </OnUpdate>
    </Scripts>
</Frame>

